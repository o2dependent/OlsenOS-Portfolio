{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"strictNullChecks": true,
		"baseUrl": "./src",
		"paths": {
			"$stores": ["./stores"],
			"$stores/*": ["./stores/*"],
			"$lib": ["./lib"],
			"$lib/*": ["./lib/*"],
			"$islands": ["./islands"],
			"$islands/*": ["./islands/*"],
			"$layouts": ["./layouts"],
			"$layouts/*": ["./layouts/*"],
			"$utils": ["./utils"],
			"$utils/*": ["./utils/*"]
		}
	}
}
